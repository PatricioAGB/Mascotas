    <div class="row" style="opacity: 0.9;">
        {{#each producto}}
        
        <div class="col-xs-6 col-sm-4 col-md-4 col-lg-3 col-xl-2 mt-4">
            <div class="card mx-auto"  style="width: 14rem;height:25rem; border:hidden; box-shadow: 1px 10px 10px 2px rgba(0, 0,0,.6);border: 6px solid transparent;border-radius: 10px;">

                <div class="card-body">
                    <a href="#">
                    
                    <img class="card-img-top" src="{{imagen}}" witdh="200" height="250">
                   
                        
                    </a>
                </div>
             <div style="text-align: center; "class="pie" height="100px">
                <div class="product-name">
                <strong class="product-item-name">
									{{nombre}}
									</strong>
              
                  </div>
                  <span data-price-amount="{{precio}}" data-price-type="finalPrice" class="price-wrapper ">
                    <span class="price"> ${{precio}} </span>    
                  </span>
                 
                </div>
            </div>
        </div>  
        {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
                <p>No se encontró ningun producto con este nombre.</p>
                <a href="/">Volver</a>
            </div>
        </div>

        {{/each}}

    </div>
</div>
